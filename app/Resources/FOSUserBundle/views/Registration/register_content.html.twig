{% trans_default_domain 'FOSUserBundle' %}
{% block divpage %}
<div class="page-inscription">{% endblock %}
    <section class="content-inscription" style="margin-top: -130px">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="cercleInscription">
                        <div class="cercleInscriptionContent">
                            <h2>Inscription</h2>
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 no-padd">
                                {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register', 'id': 'formInscription'}}) }}
                                {{ form_widget(form) }}
                                <div>
                                    <input type="submit" value="{{ 'registration.submit'|trans }}" />
                                </div>
                                {{ form_end(form) }}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>